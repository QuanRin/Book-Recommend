  "scripts": {
    "build": "nest build && npm run copy-templates",
    "copy-templates": "rm -rf dist/modules/mail/templates/ && mkdir dist/modules/mail/templates/ && cp -r src/modules/mail/templates/* dist/modules/mail/templates/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:m": "npx prisma migrate dev",
    "prisma:m-c": "npx prisma migrate dev --create-only",
    "prisma:g": "npx prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1"
  },
  "dependencies": {
    "@brakebein/prisma-generator-nestjs-dto": "1.16.0",
